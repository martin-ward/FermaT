@Test_Trans(" 1st test of `Delete_All_Redundant'",
	    (FILL Statements

DO r6 := r6 + r7;
   IF a[TVAENTRY].TVAWRK.TVAWER <= r6
     THEN r1 := !XF address_of(a[TVAENTRY].TVAMSG); EXIT(1) FI;
   IF r1 = r5
     THEN r1 := r3 - r7;
          B01();
          IF exit_flag = 1 THEN EXIT(1) FI
     ELSE C:"<FermaT> 00000585 ";
          C:"SET ITEM = ITEM + 1";
          C:"<FermaT> 00000586 ";
          r1 := r1 + 1 FI OD


ENDFILL),
	    < >,
	    TR_Delete_All_Redundant,
	    "",
	    (FILL Statements

DO r6 := r6 + r7;
   IF a[TVAENTRY].TVAWRK.TVAWER <= r6
     THEN EXIT(1) FI;
   IF r1 = r5
     THEN r1 := r3 - r7;
          B01();
          IF exit_flag = 1 THEN EXIT(1) FI
     ELSE C:"<FermaT> 00000585 ";
          C:"SET ITEM = ITEM + 1";
          C:"<FermaT> 00000586 ";
          r1 := r1 + 1 FI OD

ENDFILL));


