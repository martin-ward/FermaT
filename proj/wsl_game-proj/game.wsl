num := @Random(100);
PRINT("I have thought of a number between 1 and 100");
DO PRINFLUSH("What is your guess? ");
   guess := @String_To_Num(@Read_Line(Standard_Input_Port));
   IF guess = num THEN PRINT("Correct!"); EXIT(1) FI;
   IF guess < num
     THEN PRINT("Too low.")
     ELSE PRINT("Too high.") FI OD;
PRINT("Goodbye.")

